class Solution {
    public String reverseWords(String s) {
       //String[] s1 = s.trim();
       String res="";
       String[] s2 = s.trim().split("\\s+");
       for(int i=s2.length-1;i>=0;i--){
            res += s2[i];
            if(i != 0){
                res += " ";
            }
       }
        return res;
    }
}
